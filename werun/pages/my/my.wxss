.container{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-evenly;
}
.login-head{
  border-radius: 100px;
}
.login-head image{
  width: 240rpx;
  height: 244rpx;
  border-radius: 100px;
}
.my-list{
  width: 100%;
  height: 480rpx;
  border: 1px solid #8a8a8a;
}
.listitem{
  width: 100%;
  height: 160rpx;
  line-height: 170rpx;
  font-size: 56rpx;
  border-bottom: 1px solid #8a8a8a;
  padding-left: 90rpx;
  background-color: #cdcdce;
  color: #000;
  position: relative;
}
.triangle{
  width: 24rpx;
  height: 24rpx;
  border-top: 10rpx solid #fff;
  border-right: 10rpx solid #fff;
  transform: rotate(45deg);
  position: absolute;
  left: 610rpx;
  top: 70rpx;
}
.imgTouch{
  background-color: #ff531a;
}
.my-modal{
  border-radius: 50%;
  height:86% !important;
}
.btn-remove{
  padding: 0;
  margin: 0;
  outline: none;
  width: 131rpx !important;
}
.btn-remove::after{
  border: none;
}