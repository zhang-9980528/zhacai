<!--components/feedback/feedback.wxml-->
<view class="example-content">
    <form bindsubmit="submitFed">
    <textarea name="trtext" value="{{textarea}}" class="input-textarea" placeholder="请输入您的反馈意见"></textarea>
    <button form-type="submit">提交</button>
    </form>
    <view class="feedbackContent">
    <view wx:for="{{feedback}}" class="feedbackItem"><text>{{item.content}}</text>
    <text class="time">{{item.updatedAt}}</text>
    <view class="delete" data-id="{{item._id}}" bindtap="delete"><image src="{{delete}}"></image></view>
    </view>
    </view>
</view>

